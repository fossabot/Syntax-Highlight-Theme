// General Language Scopes file
// More info : https://macromates.com/manual/en/language_grammars#naming_conventions
// This is a beta test version, base on scope statistics
// This file is the most scopes (not all) support by vscode, only aimed to breadth first.
{
    "$schema": "vscode://schemas/color-theme",
    "name": "Common Base Scopes",
    "include": "./dark_defaults.json",
    "colors": {
        "editorLineNumber.activeForeground": "#f1610d",
        "editorLineNumber.foreground": "#0085ad",
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment.block",
                "comment.documentation",
                "comment.line",
                "comment.typehint",
                "comment.unbuffered"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#546E7A"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant.character",
                "constant.enum",
                "constant.keyword",
                "constant.language",
                "constant.name",
                "constant.numeric",
                "constant.other",
                "constant.others",
                "constant.sha",
                "constant.string",
                "constant.variable"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "Entity",
            "scope": [
                "entity.alias",
                "entity.global",
                "entity.name",
                "entity.namespace",
                "entity.other",
                "entity.scope",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Heading",
            "scope": [
                "heading.1",
                "heading.2",
                "heading.3",
                "heading.4",
                "heading.5",
                "heading.6",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid.character",
                "invalid.deprecated",
                "invalid.illeagal",
                "invalid.illegal"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword.annotation",
                "keyword.channel",
                "keyword.codetag",
                "keyword.command",
                "keyword.const",
                "keyword.control",
                "keyword.format",
                "keyword.function",
                "keyword.generator",
                "keyword.illegal",
                "keyword.import",
                "keyword.interface",
                "keyword.map",
                "keyword.module",
                "keyword.operator",
                "keyword.other",
                "keyword.package",
                "keyword.preprocessor",
                "keyword.query",
                "keyword.reserved",
                "keyword.struct",
                "keyword.symbol",
                "keyword.type",
                "keyword.typealias",
                "keyword.var",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Markup",
            "scope": [
                "markup.bold",
                "markup.changed",
                "markup.deleted",
                "markup.fenced_code",
                "markup.heading",
                "markup.inline",
                "markup.inserted",
                "markup.italic",
                "markup.list",
                "markup.quote",
                "markup.raw",
                "markup.underline",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Meta",
            "scope": [
                "meta.alter",
                "meta.angle-brackets",
                "meta.arguments",
                "meta.argument-type",
                "meta.array",
                "meta.array-binding-pattern-variable",
                "meta.arrow",
                "meta.asm",
                "meta.assertion",
                "meta.at-rule",
                "meta.attribute",
                "meta.attribute-selector",
                "meta.attribute-with-value",
                "meta.backreference",
                "meta.banner",
                "meta.block",
                "meta.block-mapping",
                "meta.body",
                "meta.brace",
                "meta.bracket",
                "meta.bracketed",
                "meta.case",
                "meta.catch",
                "meta.character",
                "meta.class",
                "meta.class-struct-block",
                "meta.closure",
                "meta.commit-command",
                "meta.commit-message",
                "meta.conditional",
                "meta.content",
                "meta.control",
                "meta.create",
                "meta.declaration",
                "meta.decorator",
                "meta.definition",
                "meta.delimiter",
                "meta.diff",
                "meta.directive",
                "meta.divider",
                "meta.drop",
                "meta.embedded",
                "meta.encoding",
                "meta.enum",
                "meta.enum-case",
                "meta.environment-variable",
                "meta.evaluation",
                "meta.even-tab",
                "meta.example",
                "meta.export",
                "meta.expression",
                "meta.extern-block",
                "meta.feature-query",
                "meta.field",
                "meta.finally",
                "meta.first-class",
                "meta.flow-mapping",
                "meta.flow-pair",
                "meta.flow-sequence",
                "meta.format",
                "meta.fstring",
                "meta.function",
                "meta.function-call",
                "meta.function-result",
                "meta.function-with-body",
                "meta.generic-argument-clause",
                "meta.generic-parameter-clause",
                "meta.generic-parameter-constraint",
                "meta.generic-where-clause",
                "meta.group",
                "meta.hashtable",
                "meta.head",
                "meta.header",
                "meta.herestring",
                "meta.id-with-protocol",
                "meta.image",
                "meta.implementation",
                "meta.import",
                "meta.import-equals",
                "meta.include",
                "meta.indexer",
                "meta.inheritance-clause",
                "meta.inheritance-list",
                "meta.inherited-class",
                "meta.initialization",
                "meta.inner-class",
                "meta.interface",
                "meta.interface-or-protocol",
                "meta.internalsubset",
                "meta.interpolation",
                "meta.item-access",
                "meta.lambda",
                "meta.lambda-function",
                "meta.link",
                "meta.map",
                "meta.member",
                "meta.metadata",
                "meta.method",
                "meta.method-call",
                "meta.module",
                "meta.named",
                "meta.namespace",
                "meta.namespace-block",
                "meta.object",
                "meta.object-binding-pattern-variable",
                "meta.objectliteral",
                "meta.object-literal",
                "meta.odd-tab",
                "meta.other",
                "meta.package",
                "meta.paragraph",
                "meta.parameter",
                "meta.parameter-clause",
                "meta.parameters",
                "meta.paramter",
                "meta.parens",
                "meta.path",
                "meta.preprocessor",
                "meta.prompt",
                "meta.property",
                "meta.property-list",
                "meta.property-name",
                "meta.property-with-attributes",
                "meta.protocol-list",
                "meta.qualified_type",
                "meta.quoted-expression",
                "meta.ratio",
                "meta.require",
                "meta.requires",
                "meta.resultBlock",
                "meta.resultLine",
                "meta.resultLinePrefix",
                "meta.return",
                "meta.return-type",
                "meta.scope",
                "meta.scope-resolution",
                "meta.scriptblock",
                "meta.section",
                "meta.selector",
                "meta.separator",
                "meta.set",
                "meta.static_assert",
                "meta.structure",
                "meta.subscript-expression",
                "meta.switch-statement",
                "meta.symbol",
                "meta.syntax",
                "meta.tag",
                "meta.tail",
                "meta.template",
                "meta.throwables",
                "meta.toc-list",
                "meta.trait",
                "meta.try",
                "meta.type",
                "meta.type_params",
                "meta.typehint",
                "meta.type-name",
                "meta.use",
                "meta.using-namespace",
                "meta.using-namespace-declaration",
                "meta.var",
                "meta.variable",
                "meta.var-single-variable",
                "meta.vector",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Markup",
            "scope": [
                "markup.bold",
                "markup.changed",
                "markup.deleted",
                "markup.fenced_code",
                "markup.heading",
                "markup.inline",
                "markup.inserted",
                "markup.italic",
                "markup.list",
                "markup.quote",
                "markup.raw",
                "markup.underline",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.access",
                "punctuation.accessor",
                "punctuation.bracket",
                "punctuation.catch",
                "punctuation.character",
                "punctuation.comment",
                "punctuation.curlybrace",
                "punctuation.decorator",
                "punctuation.definition",
                "punctuation.destructuring",
                "punctuation.other",
                "punctuation.parenthesis",
                "punctuation.range-based",
                "punctuation.section",
                "punctuation.separator",
                "punctuation.squarebracket",
                "punctuation.support",
                "punctuation.terminator",
                "punctuation.tilde",
                "punctuation.vararg-ellipses",
                "punctuation.whitespace",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage.class",
                "storage.control",
                "storage.modifier",
                "storage.type",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#C792EA"
            }
        },
        {
            "name": "String",
            "scope": [
                "string.comment",
                "string.interpolated",
                "string.json",
                "string.other",
                "string.quoted",
                "string.regexp",
                "string.template",
                "string.unquoted",
                "string.url",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Support",
            "scope": [
                "support.class",
                "support.constant",
                "support.function",
                "support.other",
                "support.type",
                "support.variable",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#B2CCD6"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable.argument",
                "variable.assignment",
                "variable.control",
                "variable.font-feature",
                "variable.function",
                "variable.interpolation",
                "variable.language",
                "variable.legacy",
                "variable.object",
                "variable.other",
                "variable.parameter",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#57c3d1"
            }
        },
    ]
}